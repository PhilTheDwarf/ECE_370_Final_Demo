<sdf version = "1.4">
<model name = "PhilRobo">
    <static>false</static>

    <!-- =============== LINKS =============== -->

    <link name = "chassis">
        <pose>0 0 0.1 0 0 0</pose>

        <collision name = "col_body">
            <geometry>
                <box>
                    <size>0.8 0.2 0.1</size>
                </box>
            </geometry>
        </collision>

        <visual name = "vis_body">
            <geometry>
                <box>
                    <size>0.8 0.2 0.1</size>
                </box>
            </geometry>
        </visual>


        <!-- caster wheel -->
        <!--
        <collision name = "col_cas">
            <pose>0 0 -0.2 0 -0.1 0</pose>
            <geometry>-->
            <!--
                <sphere>
                    <radius>0.2</radius>
                </sphere>
            --><!--
                <box>
                    <size>0.4 0.2 0.01</size>
                </box>

            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu> 0 </mu>
                        <mu2> 0 </mu2>
                        <slip1> 1.0</slip1>
                        <slip2> 1.0</slip2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <visual name="vis_cast">
            <pose>0 0 -0.2 0 -0.1 0</pose>
            <geometry>-->
                <!--
                <sphere>
                    <radius>0.2</radius>
                </sphere>
                --><!--
                <box>
                    <size>0.4 0.2 0.1</size>
                </box>
            </geometry>
        </visual>-->
    </link>



        <!--front left wheel-->

            <link name = "fl_wheel">
                <pose>0.5 0.45 0.1 0 1.5707 1.5707</pose>
                <collision name = "col_l_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu> 1.0 </mu>
                                <mu2> 1.0 </mu2>
                                <slip1> 0.0</slip1>
                                <slip2> 0.0</slip2>
                            </ode>
                        </friction>
                    </surface>
                </collision>

                <visual name = "vis_l_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                </visual>
            </link>

        <!--back left wheel-->

            <link name = "bl_wheel">
                <pose>-0.25 0.45 0.1 0 1.5707 1.5707</pose>
                <collision name = "col_l_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu> 1.0 </mu>
                                <mu2> 1.0 </mu2>
                                <slip1> 0.0</slip1>
                                <slip2> 0.0</slip2>
                            </ode>
                        </friction>
                    </surface>
                </collision>

                <visual name = "vis_l_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                </visual>
            </link>
            


        <!--right wheel-->

            <link name = "fr_wheel">
                <pose>0.25 -0.45 0.1 0 1.5707 1.5707</pose>
                <collision name = "col_r_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu> 1.0 </mu>
                                <mu2> 1.0 </mu2>
                                <slip1> 0.0</slip1>
                                <slip2> 0.0</slip2>
                            </ode>
                        </friction>
                    </surface>
                </collision>

                <visual name = "vis_r_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                </visual>
            </link>

        <!--right wheel-->

            <link name = "br_wheel">
                <pose>-0.25 -0.45 0.1 0 1.5707 1.5707</pose>
                <collision name = "col_r_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                    <surface>
                        <friction>
                            <ode>
                                <mu> 1.0 </mu>
                                <mu2> 1.0 </mu2>
                                <slip1> 0.0</slip1>
                                <slip2> 0.0</slip2>
                            </ode>
                        </friction>
                    </surface>
                </collision>

                <visual name = "vis_r_wheel">
                    <geometry>
                        <cylinder>
                            <radius>0.24</radius>
                            <length>0.05</length>
                        </cylinder>
                    </geometry>
                </visual>
            </link>



        <!-- =============== JOINTS =============== -->


        <joint type = "revolute" name = "hinge_fl_wheel">
            <pose>0 0 -0.03 0 0 0</pose>
            <child>fl_wheel</child>
            <parent> chassis </parent>
            <axis>
                <xyz> 0 1 0 </xyz>
            </axis>
            <dynamics>
                <friction>100.0</friction>
                <spring_stiffness>0.1</spring_stiffness>
            </dynamics>
        </joint>

        <joint type = "revolute" name = "hinge_bl_wheel">
            <pose>0 0 -0.03 0 0 0</pose>
            <child>bl_wheel</child>
            <parent> chassis </parent>
            <axis>
                <xyz> 0 1 0 </xyz>
            </axis>
            <dynamics>
                <friction>100.0</friction>
                <spring_stiffness>0.1</spring_stiffness>
            </dynamics>
        </joint>

        
        <joint type = "revolute" name = "hinge_r_wheel">
            <pose>0 0 0.03 0 0 0</pose>
            <child>r_wheel</child>
            <parent> chassis </parent>
            <axis>
                <xyz> 0 1 0 </xyz>
            </axis>
            <dynamics>
                <friction>100.0</friction>
                <spring_stiffness>0.1</spring_stiffness>
            </dynamics>
        </joint>

    </model>
</sdf>